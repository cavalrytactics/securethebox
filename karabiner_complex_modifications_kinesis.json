          "from": {
            "key_code": "w",
            "modifiers": {
              "mandatory": [
                "command"
              ]
            }
          },
          "to": [
            {
              "key_code": "w",
              "modifiers": "left_command"
            }
          ]
        },
        {
          "type": "basic",
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^io\\.alacritty$"
              ]
            }
          ],
          "from": {
            "key_code": "w",
