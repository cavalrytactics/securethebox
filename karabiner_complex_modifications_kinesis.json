{
  "title": "Prevent unintended shit",
  "rules": [
    {
      "description": "Left ctrl + left shift + hjkl to arrow keys Vim",
      "manipulators": [
        {
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": [
                "left_command",
                "left_shift"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": [
                "left_command",
                "left_shift"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "down_arrow"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": [
                "left_command",
                "left_shift"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "up_arrow"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": [
                "left_command",
                "left_shift"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_arrow"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "backspace to left_command",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "delete_or_backspace",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_command",
              "repeat": true
            }
          ],
          "to_if_alone": [
            {
              "key_code": "delete_or_backspace",
              "repeat": false
            }
          ],
          "to_if_held_down": [
            {
              "key_code": "left_command",
              "repeat": true
            }
          ]
        }
      ]
    },
    {
      "description": "spacebar to right_control",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_control",
              "repeat": true
            }
          ],
          "to_if_alone": [
            {
              "key_code": "spacebar",
              "repeat": false
            }
          ],
          "to_if_held_down": [
            {
              "key_code": "right_control",
              "repeat": true
            }
          ]
        }
      ]
    },
    {
      "description": "right_control + left_command + semicolon to underscore",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "semicolon",
            "modifiers": {
              "mandatory": [
                "right_control",
                "left_command"
              ]
            }
          },
          "to_if_alone": [
            {
              "key_code": "hyphen",
              "modifiers": "left_shift"
            }
          ]
        }
      ]
    },
    {
      "description": "right_control + left_command + a to plus",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "a",
            "modifiers": {
              "mandatory": [
                "right_control",
                "left_command"
              ]
            }
          },
          "to_if_alone": [
            {
              "key_code": "equal_sign",
              "modifiers": "left_shift"
            }
          ]
        }
      ]
    },
    {
      "description": "right_control + left_command + h to hyphen",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": [
                "right_control",
                "left_command"
              ]
            }
          },
          "to_if_alone": [
            {
              "key_code": "hyphen"
            }
          ]
        }
      ]
    },
    {
      "description": "right_control + left_command + g to equal",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "g",
            "modifiers": {
              "mandatory": [
                "right_control",
                "left_command"
              ]
            }
          },
          "to_if_alone": [
            {
              "key_code": "equal_sign"
            }
          ]
        }
      ]
    },
    {
      "description": "right_control + left_command + f to open parenthesis",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f",
            "modifiers": {
              "mandatory": [
                "right_control",
                "left_command"
              ]
            }
          },
          "to_if_alone": [
            {
              "key_code": "9",
              "modifiers": "left_shift"
            }
          ]
        }
      ]
    },
    {
      "description": "right_control + left_command + j to close parenthesis",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": [
                "right_control",
                "left_command"
              ]
            }
          },
          "to": [
            {
              "key_code": "0",
              "modifiers": "left_shift"
            }
          ]
        }
      ]
    },
    {
      "description": "right_control + left_command + d to open curly bracket",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "d",
            "modifiers": {
              "mandatory": [
                "right_control",
                "left_command"
              ]
            }
          },
          "to": [
            {
              "key_code": "open_bracket",
              "modifiers": "left_shift"
            }
          ]
        }
      ]
    },
    {
      "description": "right_control + left_command + k to close curly bracket",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": [
                "right_control",
                "left_command"
              ]
            }
          },
          "to": [
            {
              "key_code": "close_bracket",
              "modifiers": "left_shift"
            }
          ]
        }
      ]
    },
    {
      "description": "left_command + right_control + s to open box bracket",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "s",
            "modifiers": {
              "mandatory": [
                "right_control",
                "left_command"
              ]
            }
          },
          "to": [
            {
              "key_code": "open_bracket"
            }
          ]
        }
      ]
    },
    {
      "description": "left_command + right_control + l to close box bracket",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": [
                "right_control",
                "left_command"
              ]
            }
          },
          "to": [
            {
              "key_code": "close_bracket"
            }
          ]
        }
      ]
    },
    {
      "description": "Quit App by pressing command-q twice then hold",
      "manipulators": [
        {
          "type": "basic",
          "conditions": [
            {
              "type": "variable_if",
              "name": "command-q",
              "value": 1
            },
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^io\\.alacritty$"
              ]
            }
          ],
          "from": {
            "key_code": "q",
            "modifiers": {
              "mandatory": [
                "command"
              ]
            }
          },
          "to": [
            {
              "key_code": "q",
              "modifiers": "left_command"
            }
          ]
        },
        {
          "type": "basic",
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^io\\.alacritty$"
              ]
            }
          ],
          "from": {
            "key_code": "q",
            "modifiers": {
              "mandatory": [
                "command"
              ]
            }
          },
          "to_if_held_down": [
            {
              "key_code": "q",
              "modifiers": [
                "left_command"
              ],
              "repeat": false
            }
          ]
        }
      ]
    },
