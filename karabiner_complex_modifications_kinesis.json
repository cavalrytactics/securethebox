            },
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^io\\.alacritty$"
              ]
            }
          ],
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": [
                "command"
              ]
            }
          },
          "to": [
            {
              "key_code": "h",
